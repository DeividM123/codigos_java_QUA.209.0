package principal;

public class variaveis {

	public static void main(String[] args) {
		int numero;
		numero = 10;

		double decimal = 10;
		int testeNumero = (int) 50.25;
		double testeNumeroNovo = 10;

		String nome = "Luan Muruk";
		char sexo = 'F';
		boolean verdadeiro = true;
		boolean falso = false;
		double peso = 100.00;
		int idade = 5;
		System.out.println(nome);
		System.out.println(testeNumeroNovo);
		System.out.println(decimal);
		System.out.print("Nome: ");
		System.out.println(nome);

		System.out.println("****************************");
		System.out.print("Meu nome é: "+nome);
	}

}
